{
  "name": "gone-busy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "autoprefixer": "postcss -u autoprefixer -b 'last 2 versions' -r public/css/*",
    "scss": "node-sass --output-style compressed -o public/css src/scss/",
    "build:css": "npm run scss && npm run autoprefixer",
    "watch:css": "onchange 'src/scss/**/*.scss' -- npm run build:css",
    "watch:js": "webpack --progress --color --watch",
    "build:js": "NODE_ENV=production webpack --progress --color -p",
    "dev": "npm run start & npm run watch:js & npm run watch:css",
    "build": "npm run build:js & npm run build:css"
  },
  "author": "Richard Lehmann",
  "private": true,
  "dependencies": {
    "bhttp": "^1.2.1",
    "bluebird": "^3.4.0",
    "body-parser": "^1.13.3",
    "bootstrap": "^3.3.6",
    "bootstrap-validator": "^0.10.2",
    "gsap": "^1.18.5",
    "jquery": "^2.1.4",
    "request": "^2.72.0",
    "url-join": "^1.1.0",
    "waypoints": "^4.0.0",
    "webfontloader": "^1.6.22"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.6",
    "chai": "^3.5.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "hbs": "~3.1.0",
    "morgan": "~1.6.1",
    "node-sass": "^3.7.0",
    "onchange": "^2.4.0",
    "postcss": "^5.0.16",
    "postcss-cli": "^2.5.1",
    "sass-loader": "^3.2.0",
    "serve-favicon": "~2.3.0",
    "webpack": "^1.13.1"
  }
}
